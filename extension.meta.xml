	<repo type="github">https://github.com/jonmifsud/tag_ui_selector</repo>
	<types>
		<type>Interface</type>
	</types>
	<authors>
		<author>
			<name github="jonmifsud" symphony="gunglien">Jon Mifsud</name>
			<website>http://jonmifsud.com</website>
		</author>
	</authors>
	<dependencies>
		<dependency version="1.1.1">association_ui_selector</dependency>
	</dependencies>
	<releases>
		<release version="1.0.0" date="2014-08-13" min="2.5">
			- First release
			- Based on Nils Association UI: Selector
		</release>
	</releases>
</extension>
